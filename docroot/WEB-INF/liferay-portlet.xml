<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.0.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_0_0.dtd">

<liferay-portlet-app>

	<portlet>
		<portlet-name>consoleadminportlet</portlet-name>
		<icon>/icon_small.png</icon>
		<control-panel-entry-category>server</control-panel-entry-category>
		<preferences-company-wide>true</preferences-company-wide>
		<preferences-unique-per-layout>true</preferences-unique-per-layout>
		<preferences-owned-by-group>true</preferences-owned-by-group>
		<instanceable>false</instanceable>
		<css-class-wrapper>consoleadmin-portlet</css-class-wrapper>
		<system>true</system>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>